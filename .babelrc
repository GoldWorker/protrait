{
    "presets": [
        ["env", {
            "targets": {
                "browsers": [
                    ">0.25%",
                    "not ie 11",
                    "not op_mini all"
                ]
            },
            "modules": false
        }],
        "react",
        "stage-0"
    ],
    "plugins": [
        ["import",
            {
                "libraryName": "slucky",
                "libraryDirectory": "lib/component",
            },
            {
                "libraryName": "antd",
                "libraryDirectory": "es"
            }
        ],
        "react-hot-loader/babel",
        "syntax-dynamic-import",
        [
            "transform-runtime",
            {
                "polyfill": false,
            }
        ]
    ]
}
